<odoo>
    <data>
        <template id="report_completed_tasks">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="task">
                    <div>
                        <h2><t t-esc="task.nombre"/></h2>
                        <p><t t-esc="task.descripcion"/></p>
                    </div>
                </t>
            </t>
        </template>
    </data>
</odoo>
